{
  "QpSettings": {
    "SiteId": 52,
    "IsStage": true,
    "ConnectionString": "Application Name=DemoSite;Initial Catalog=__db.name__;Data Source=__db.server__;User ID=__db.user__;Password=__db.password__;",
    "DatabaseType": "__qp.databasetype__",
    "CustomerCode": "__qp.customercode__",
    "ConfigurationServiceUrl": "__configurationservice.url__",
    "ConfigurationServiceToken": "__configurationservice.token__"
  },
  "QpSiteStructureSettings": {
    "CachePeriod": "00:10:00",
    "RootPageDiscriminator": "root_page",
    "UploadUrlPlaceholder": "<%=upload_url%>",
    "LoadM2mForAbstractItem": true,
    "LoadAbstractItemFieldsToDetailsCollection": true
  },
  "OnScreen": {
    "AdminSiteBaseUrl": "__settings.onscreenUrl__"
  },
  "UrlTokenConfig": {
    "MatchingPatterns": [
      {
        "Value": "/{culture}/{region}"
      },
      {
        "Value": "/{region}",
        "Defaults": {
          "culture": "ru-ru"
        }
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Information"
    }
  }
}
