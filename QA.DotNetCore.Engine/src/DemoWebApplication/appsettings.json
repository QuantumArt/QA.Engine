{
  "QpSettings": {
    "SiteId": 52,
    "IsStage": true
  },
  //"QpSchemeCacheSettings": {
  //  "CachePeriod": "00:00:30"
  //},
  //"ItemDefinitionCacheSettings": {
  //  "CachePeriod": "00:00:30"
  //},
  "QpSiteStructureSettings": {
    "CachePeriod": "00:10:00",
    "RootPageDiscriminator": "root_page",
    "UploadUrlPlaceholder": "<%=upload_url%>",
    "LoadM2mForAbstractItem": true,
    "LoadAbstractItemFieldsToDetailsCollection": true
  },
  "ConnectionStrings": {
    "QpConnection": "Application Name=DemoSite;Initial Catalog=__db.name__;Data Source=__db.server__;User ID=__db.user__;Password=__db.password__;"
  },
  "OnScreen": {
    "AdminSiteBaseUrl": "__settings.onscreenUrl__"
  },
  "UrlTokenConfig": {
    "MatchingPatterns": [
      {
        "Value": "/{culture}/{region}"
      },
      {
        "Value": "/{region}",
        "Defaults": {
          "culture": "ru-ru"
        }
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Information"
    }
  }
}
